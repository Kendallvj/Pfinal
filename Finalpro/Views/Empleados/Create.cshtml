@model Finalpro.Models.Empleados

<h1>Nuevo Empleado</h1>

<form asp-action="Create">
    <div class="form-group">
        <label>Nombre</label>
        <input type="text" class="form-control" asp-for="NombreEmpleado" />
    </div>
    <div class="form-group">
        <label>Apellido</label>
        <input type="text" class="form-control" asp-for="ApellidoEmpleado" />
    </div>
    

    <button type="submit" class="btn btn-primary">Guardar</button>
</form>
