@model Finalpro.Models.Ortodoncia

<h1>Confirmar Eliminación</h1>

<div>
    <h4>Id Paciente: @Model.IdPaciente</h4>
    <h4>Nombre Paciente: @Model.NombrePaciente</h4>
    <h4>Edad Paciente: @Model.EdadPaciente</h4>
    <h4>Seguro Paciente: @Model.SeguroPaciente</h4>
    
</div>
<form asp-action="Delete">
    <input type="hidden" asp-for="IdPaciente" />
    <button type="submit" class="btn btn-danger">Confirmar Eliminación</button> |
    <a asp-action="Index">Cancelar</a>
</form>
