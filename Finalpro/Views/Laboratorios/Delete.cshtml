@model Finalpro.Models.Laboratorio

<h1>Confirmar Eliminación</h1>

<div>
    <h4>Id: @Model.IdLaboratorio</h4>
    <h4>Nombre: @Model.NombreLab</h4>
    <h4>Dirección: @Model.DireccionLab</h4>
    <h4>Teléfono: @Model.TelefonoLab</h4>
    <h4>Email: @Model.EmailLab</h4>
    <h4>Encargado de Área: @Model.EncargadoArea</h4>
    <h4>Id Equipo Odontológico: @Model.IdEquipoOdonto</h4>
    <h4>Tipo de Análisis: @Model.TipoAnalisis</h4>
    
</div>
<form asp-action="Delete">
    <input type="hidden" asp-for="IdLaboratorio" />
    <button type="submit" class="btn btn-danger">Confirmar Eliminación</button> |
    <a asp-action="Index">Cancelar</a>
</form>
